    "fs-extra": "7.0.0",
    "mocha": "5.2.0",
    "mock-fs": "4.7.0",
    "replace": "1.0.0",
    "sinon": "6.3.4",
    "sinon-chai": "3.2.0"
  },
  "devDependencies": {
    "@chimeraprime/eslint-config-chimera-prime": "1.0.4",
    "eslint": "5.6.0"
  }
}
